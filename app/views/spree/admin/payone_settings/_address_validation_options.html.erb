<div id="<%= address_validation_address_check %>">
  <%  key = address_check_address_check_type
      type = Spree::Config.preference_type(key)
      options = address_validation_preferences[key] %>
  <div class="form-group">
    <%= label_tag key, t(key, scope: 'payone') %>
    <%= payone_settings_preference_field_tag(key, Spree::Config[key], options.merge!(type: type)) %>
  </div>
</div>
